<%- include('./layouts/topDashboard'); %>
<h4>content order</h4>
<a href="/dashboard/pesanan">
    link ke form order
</a>
<div>
    <table class="table table-dark">
        <thead>
            <tr>
                <th>Makanan</th>
                <th>Price</th>
                <th>Pemesan</th>
                <th>status</th>
                <th>Time</th>
                <th>Bayar</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i=0;i<list.length;i++) { %> 
            <tr>
                    <td><%= list[i].foods %></td>
                    <td><%= list[i].price %></td>
                    <td><%= list[i].pemesan %></td>
                    <td><%= list[i].status %></td>
                    <td><%= list[i].dateOrder %></td>
                    <td>
                        <a href="/dashboard/payment/<%= list[i].id %>">
                            <button class="btn btn-outline-success"  > Bayar </button>
                        </a>
                        
                    </td>
            </tr>
            <% } %>
        </tbody>
    </table>
</div>
<%- include('./layouts/footerDashboard'); %>